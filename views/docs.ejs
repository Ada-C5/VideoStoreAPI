<!DOCTYPE html>
<html lang="en">
<head>
  <title>VideoStoreAPI Documentation</title>
  <meta charset="utf-8">
</head>

<body>
  <header>
    <h1>VideoStoreAPI</h1>
    <h2>API Documentation</h2>

    <% for (var top_endpoint of Object.keys(documentation)) { %>
      <%= top_endpoint %>
    <% } %>

    <br><br>


    <% for (var endpoint of documentation.endpoints) { %>
      Endpoint:<br>
      <% for (var info of Object.keys(endpoint)) { %>
        <%= info %>:<br>
        <%= endpoint[info] %><br>
      <% } %>

    <% } %>
  </header>
</body>

</html>
