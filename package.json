{
  "name": "VideoStoreAPI",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "node ./setup.js",
    "start": "./node_modules/.bin/nodemon ./bin/www",
    "start-test": "NODE_ENV=test ./node_modules/.bin/nodemon ./bin/www",
    "test": "clear; ./node_modules/.bin/istanbul cover --include-all-sources ./node_modules/.bin/jasmine-node --verbose spec/",
    "db:drop": "dropdb radio_star_development",
    "db:create": "createdb radio_star_development",
    "db:dropt": "dropdb radio_star_test",
    "db:createt": "createdb radio_star_test",
    "db:schema": "node tasks/load_schema.js",
    "db:seed": "node tasks/seed_data.js",
    "db:reset": "npm run db:drop; npm run db:dropt; npm run db:create; npm run db:createt; npm run db:schema; npm run db:schema; npm run db:seed",
    "db:all": "npm run setup; npm run db:reset_development; npm run db:reset_test "
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "massive": "^2.3.0",
    "morgan": "~1.6.1",
    "nodemon": "^1.9.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.4",
    "jasmine-node": "^1.14.5",
    "massive": "^2.3.0",
    "nodemon": "^1.9.2",
    "request": "^2.72.0"
  }
}
