{
  "name": "video-store-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "start-test": "NODE_ENV=test ./node_modules/.bin/nodemon ./bin/www",
    "test": "clear; ./node_modules/.bin/jasmine-node --captureExceptions --verbose spec/",
    "db:drop": "dropdb video_store",
    "db:create": "createdb video_store",
    "db:schema": "node tasks/load_schema.js",
    "db:seed": "node tasks/seed.js",
    "reset": "npm run db:drop; npm run db:create; npm run db:schema; npm run db:seed;"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "massive": "^2.3.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.4",
    "jasmine-node": "^1.14.5",
    "massive": "^2.3.0",
    "nodemon": "^1.9.2",
    "request": "^2.72.0"
  }
}
